<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.ashishvz.rcubsyllabus.Fourthsem">

    <Button
        android:id="@+id/button29"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/daa"
        ads:layout_constraintBottom_toTopOf="@+id/button30"
        ads:layout_constraintHorizontal_bias="0.5"
        ads:layout_constraintLeft_toLeftOf="parent"
        ads:layout_constraintRight_toRightOf="parent"
        ads:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/button30"
        app:layout_constraintStart_toStartOf="@+id/button30" />

    <Button
        android:id="@+id/button30"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/java"
        ads:layout_constraintBottom_toTopOf="@+id/button31"
        ads:layout_constraintHorizontal_bias="0.5"
        ads:layout_constraintLeft_toLeftOf="parent"
        ads:layout_constraintRight_toRightOf="parent"
        ads:layout_constraintTop_toBottomOf="@+id/button29"
        app:layout_constraintEnd_toEndOf="@+id/button31"
        app:layout_constraintStart_toStartOf="@+id/button31" />

    <Button
        android:id="@+id/button31"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/vb"
        ads:layout_constraintBottom_toTopOf="@+id/button32"
        ads:layout_constraintHorizontal_bias="0.5"
        ads:layout_constraintLeft_toLeftOf="parent"
        ads:layout_constraintRight_toRightOf="parent"
        ads:layout_constraintTop_toBottomOf="@+id/button30"
        app:layout_constraintEnd_toEndOf="@+id/button32"
        app:layout_constraintStart_toStartOf="@+id/button32" />

    <Button
        android:id="@+id/button32"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/it"
        ads:layout_constraintBottom_toTopOf="@+id/button33"
        ads:layout_constraintHorizontal_bias="0.5"
        ads:layout_constraintLeft_toLeftOf="parent"
        ads:layout_constraintRight_toRightOf="parent"
        ads:layout_constraintTop_toBottomOf="@+id/button31"
        app:layout_constraintEnd_toEndOf="@+id/button33"
        app:layout_constraintStart_toStartOf="@+id/button33" />

    <Button
        android:id="@+id/button33"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/se"
        ads:layout_constraintBottom_toTopOf="@+id/adView6"
        ads:layout_constraintHorizontal_bias="0.5"
        ads:layout_constraintLeft_toLeftOf="parent"
        ads:layout_constraintRight_toRightOf="parent"
        ads:layout_constraintTop_toBottomOf="@+id/button32"
        app:layout_constraintEnd_toEndOf="@+id/adView6"
        app:layout_constraintStart_toStartOf="@+id/adView6" />

    <com.google.android.gms.ads.AdView
        android:id="@+id/adView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        ads:layout_constraintBottom_toBottomOf="parent"
        ads:layout_constraintHorizontal_bias="0.5"
        ads:layout_constraintTop_toBottomOf="@+id/button33"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        ads:adSize="BANNER"
        ads:adUnitId="@string/tempadid"/>
</android.support.constraint.ConstraintLayout>
